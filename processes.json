{
    "apps": [
        {
            "name": "example-ssr",
            "script": "./server/index.js",
            "env": {
                "NODE_ENV": "development"
            },
            "env_": {
                "NODE_ENV": "production"
            },
            "max_memory_restart": "300M",
            "env_production": {
                "NODE_ENV": "production"
            },
            "error_file": "../logs/wap/err.log",
            "out_file": "../logs/wap/out.log",
            "instances": 0,
            "exec_mode": "cluster",
            "merge_logs": true,
            "log_date_format": "\\[YYYY-MM-DD HH:mm:ss\\]"
        }
    ]
}
