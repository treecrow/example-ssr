{
    "apps": [
        {
            "name": "example-ssr",
            "script": "./server/index.js",
            "watch": true,
            "exec_mode": "cluster",
            "instances": 2,
            "env": {
                "NODE_ENV": "development"
            },
            "env_production": {
                "NODE_ENV": "production"
            },
            "max_memory_restart": "300M",
            "time": true,
            "error_file": "./logs/pm-error.log",
            "out_file": "./logs/pm-out.log"
        }
    ]
}
